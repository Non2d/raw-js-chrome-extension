{
  "name": "Debate Structure Visualizer",
  "description": "My Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://debatedata.io/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": [ "<all_urls>" ]
    }
  ]
}